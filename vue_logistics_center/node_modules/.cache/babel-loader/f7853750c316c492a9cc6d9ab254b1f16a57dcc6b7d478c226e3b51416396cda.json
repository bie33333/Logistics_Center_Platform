ame = '';\r\n        },\r\n        addAccount(){\r\n            this.form.account='';\r\n            this.centerDialogVisible = true;\r\n            this.$nextTick(() => {\r\n            this.resetForm();\r\n          })\r\n        },\r\n        resetForm() {\r\n            this.$refs.form.resetFields();\r\n        },\r\n\r\n        update(row){\r\n          this.centerDialogVisible = true;\r\n          this.$nextTick(() => {\r\n            this.resetForm();\r\n            this.form.name = row.name;\r\n            this.form.account = row.account;\r\n            this.form.password = row.password;\r\n            this.form.phone = row.phone;\r\n            this.form.age = row.age;\r\n            this.form.sex = row.sex;\r\n            this.form.address = row.address;\r\n          }) \r\n        }\r\n    },\r\n\r\n    mounted: {\r\n    },\r\n}\r\n</script>\r\n\r\n<style>\r\n.mgb20 {\r\n    margin-bottom: 20px;\r\n}\r\n</style>"],"mappings":"AA8EA,SAAAA,UAAA,EAAAC,QAAA,EAAAC,QAAA,EAAAC,SAAA;AACA;EACAC,KAAA;IACA;MACAC,QAAA;MACAC,SAAA;MACAC,mBAAA;MACAC,IAAA,EAAAN,QAAA;MACAO,KAAA,EAAAR,QAAA;IACA;EACA;EACAS,QAAA;IACAC,eAAA;MAAAC,UAAA;MAAAC,QAAA;IAAA,GAAAC,IAAA,CAAAC,GAAA;MACAC,OAAA,CAAAC,GAAA,CAAAF,GAAA;MACA;IACA;EACA;;EACAG,OAAA;IACAC,WAAA;MACA,KAAAd,QAAA;IACA;IACAe,WAAA;MACA,KAAAZ,IAAA,CAAAa,OAAA;MACA,KAAAd,mBAAA;MACA,KAAAe,SAAA;QACA,KAAAC,SAAA;MACA;IACA;IACAA,UAAA;MACA,KAAAC,KAAA,CAAAhB,IAAA,CAAAiB,WAAA;IACA;IAEAC,OAAAC,GAAA;MACA,KAAApB,mBAAA;MACA,KAAAe,SAAA;QACA,KAAAC,SAAA;QACA,KAAAf,IAAA,CAAAoB,IAAA,GAAAD,GAAA,CAAAC,IAAA;QACA,KAAApB,IAAA,CAAAa,OAAA,GAAAM,GAAA,CAAAN,OAAA;QACA,KAAAb,IAAA,CAAAqB,QAAA,GAAAF,GAAA,CAAAE,QAAA;QACA,KAAArB,IAAA,CAAAsB,KAAA,GAAAH,GAAA,CAAAG,KAAA;QACA,KAAAtB,IAAA,CAAAuB,GAAA,GAAAJ,GAAA,CAAAI,GAAA;QACA,KAAAvB,IAAA,CAAAwB,GAAA,GAAAL,GAAA,CAAAK,GAAA;QACA,KAAAxB,IAAA,CAAAyB,OAAA,GAAAN,GAAA,CAAAM,OAAA;MACA;IACA;EACA;EAEAC,OAAA,GACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}