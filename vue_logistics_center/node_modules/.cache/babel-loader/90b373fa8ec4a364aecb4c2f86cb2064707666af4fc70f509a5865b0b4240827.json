{"ast":null,"code":"import { carList, carRules, carForm, statusList, selectCar, addCar, modifyCar, deleteCar, updateCar } from \"@/js/car.js\";\nimport { handleRes, handleAddButton, handleUpdateButton } from \"@/utils/common.js\";\nexport default {\n  data() {\n    return {\n      name: '',\n      status: '',\n      tableData: [],\n      status: statusList(),\n      addDialogVisible: false,\n      updateDialogVisible: false,\n      form: carForm(),\n      rules: '',\n      pageSet: {\n        pageNumber: 0,\n        pageSize: 30,\n        pageTotal: 0\n      }\n    };\n  },\n  created() {\n    this.rules = carRules(this.form);\n    this.selectCars();\n  },\n  methods: {\n    selectCars() {\n      selectCar(this.pageSet).then(res => {\n        this.tableData = res.data.list;\n      });\n    },\n    //重置搜索框\n    resetParam() {\n      this.name = '';\n      this.status = '';\n    },\n    //删除操作\n    del(row) {\n      this.$confirm('确定要删除此车辆吗？', '提示', {\n        type: 'warning'\n      }).then(() => {\n        deleteCar(row).then(handleRes(this, \"删除成功\", this.selectCars));\n      }).catch(() => {});\n    },\n    /**\r\n     * 表单操作\r\n     */\n    //增加车辆\n    handleAdd() {\n      addCar(this.form).then(handleRes(this, \"增加车辆成功\", this.selectCars));\n    },\n    handleUpdate() {\n      updateCar(this.form).then(handleRes(this, \"修改成功\", this.selectCars));\n    },\n    //添加按钮\n    add() {\n      handleAddButton(this);\n    },\n    //修改按钮\n    update(row) {\n      handleUpdateButton(this, row);\n    }\n  },\n  mounted: {}\n};","map":{"version":3,"names":["carList","carRules","carForm","statusList","selectCar","addCar","modifyCar","deleteCar","updateCar","handleRes","handleAddButton","handleUpdateButton","data","name","status","tableData","addDialogVisible","updateDialogVisible","form","rules","pageSet","pageNumber","pageSize","pageTotal","created","selectCars","methods","then","res","list","resetParam","del","row","$confirm","type","catch","handleAdd","handleUpdate","add","update","mounted"],"sources":["src/components/page/Car.vue"],"sourcesContent":["<template>\r\n    <div style=\"margin-bottom: 5px;margin-top: 5px;border-radius: 30%;\">\r\n        <el-input v-model=\"name\" placeholder=\"请输入车辆名称关键字\" suffix-icon=\"el-icon-search\" style=\"width: 300px;\"></el-input>\r\n    \r\n        <el-select v-model=\"status\" filterable placeholder=\"请选择车辆状态\" style=\"margin-left: 10px;\">\r\n          <el-option\r\n                v-for=\"item in status\"\r\n                :key=\"item.value\"\r\n                :label=\"item.label\"\r\n                :value=\"item.value\">\r\n          </el-option>\r\n        </el-select>\r\n    \r\n        <el-button type=\"success\" style=\"margin-left: 10px;\">搜索</el-button>\r\n        <el-button type=\"info\" @click=\"resetParam\">重置</el-button>\r\n        <el-button size=\"medium\" type=\"primary\" style=\"margin-left: 10px;\" @click=\"add\">增添新车辆</el-button>\r\n      \r\n        <div>\r\n          <el-table :data=\"tableData\" :header-cell-style=\"{ background:'orange',color:'black'}\" border>\r\n            <el-table-column prop=\"id\" label=\"车辆编号\" width=\"180\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"name\" label=\"车辆名称\" width=\"120\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"status\" label=\"车辆状态\" width=\"120\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"describe\" label=\"车辆描述\" width=\"500\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"operate\" label=\"操作\">\r\n              <template slot-scope=\"scope\">\r\n                <el-button type=\"success\" @click=\"update(scope.row)\">修改</el-button>\r\n                <!-- <el-button type=\"success\" @click=\"handleUpdateButton(this, scope.row)\">修改</el-button> -->\r\n                <el-button type=\"danger\" @click=\"del(scope.row)\">删除</el-button>\r\n              </template>\r\n            </el-table-column>\r\n          </el-table>\r\n        </div>\r\n      \r\n        \r\n        <!-- 增添表单 -->\r\n        <el-dialog ref=\"add\"\r\n          title=\"\"\r\n          :visible.sync=\"addDialogVisible\"\r\n          width=\"50%\"\r\n          center>\r\n          <el-form ref=\"form\" :rules=\"rules\" :model=\"form\" label-width=\"100px\">\r\n            <el-form-item label=\"车辆编号\" prop=\"id\">\r\n              <el-input v-model=\"form.id\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"车辆名称\" prop=\"name\">\r\n                <el-input v-model=\"form.name\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"车辆状态\" prop=\"status\">\r\n                <el-select v-model=\"form.status\" placeholder=\"车辆状态\">\r\n                  <el-option label=\"待出车\" value=\"0\"></el-option>\r\n                  <el-option label=\"出车中\" value=\"1\"></el-option>\r\n                </el-select>\r\n            </el-form-item>\r\n            <el-form-item label=\"物品描述\" prop=\"describe\">\r\n              <el-input v-model=\"form.describe\"></el-input>\r\n            </el-form-item>\r\n          </el-form>\r\n          <span slot=\"footer\" class=\"dialog-footer\">\r\n            <el-button @click=\"addDialogVisible = false\">取消</el-button>\r\n            <el-button type=\"primary\" @click=\"handleAdd\">确定</el-button>\r\n          </span>\r\n        </el-dialog>\r\n\r\n        <!-- 增添表单 -->\r\n        <el-dialog ref=\"update\"\r\n          title=\"\"\r\n          :visible.sync=\"updateDialogVisible\"\r\n          width=\"50%\"\r\n          center>\r\n          <el-form ref=\"form\" :rules=\"rules\" :model=\"form\" label-width=\"100px\">\r\n            <el-form-item label=\"车辆编号\" prop=\"id\">\r\n              <el-input v-model=\"form.id\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"车辆名称\" prop=\"name\">\r\n                <el-input v-model=\"form.name\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"车辆状态\" prop=\"status\">\r\n                <el-select v-model=\"form.status\" placeholder=\"请选择仓库\">\r\n                  <el-option label=\"待出车\" value=\"0\"></el-option>\r\n                  <el-option label=\"出车中\" value=\"1\"></el-option>\r\n                </el-select>\r\n            </el-form-item>\r\n            <el-form-item label=\"物品描述\" prop=\"describe\">\r\n              <el-input v-model=\"form.describe\"></el-input>\r\n            </el-form-item>\r\n          </el-form>\r\n          <span slot=\"footer\" class=\"dialog-footer\">\r\n            <el-button @click=\"updateDialogVisible = false\">取消</el-button>\r\n            <el-button type=\"primary\" @click=\"handleUpdate\">确定</el-button>\r\n          </span>\r\n        </el-dialog>\r\n      </div>\r\n</template>\r\n\r\n<script>\r\nimport { carList,carRules,carForm,statusList,selectCar,addCar, modifyCar, deleteCar, updateCar } from \"@/js/car.js\";\r\nimport { handleRes, handleAddButton, handleUpdateButton } from \"@/utils/common.js\";\r\nexport default {\r\n    data() {\r\n      return {\r\n          name:'',\r\n          status:'',\r\n          tableData: [],\r\n          status: statusList(),\r\n          addDialogVisible: false,\r\n          updateDialogVisible: false,\r\n          form: carForm(),\r\n          rules: '',\r\n          pageSet: {\r\n            pageNumber: 0,\r\n            pageSize: 30,\r\n            pageTotal: 0,\r\n          }\r\n      }\r\n},\r\ncreated() {\r\n  this.rules = carRules(this.form);\r\n  this.selectCars();\r\n},\r\n  methods: {\r\n    selectCars() {\r\n      selectCar(this.pageSet).then(res => {\r\n        this.tableData = res.data.list;\r\n      })\r\n    },\r\n    //重置搜索框\r\n    resetParam(){\r\n        this.name = '';\r\n        this.status = '';\r\n    },\r\n    //删除操作\r\n    del(row) {\r\n      this.$confirm('确定要删除此车辆吗？', '提示', {type: 'warning'}).then(()=>{\r\n        deleteCar(row).then(handleRes(this,\"删除成功\",this.selectCars))\r\n      }).catch(()=>{});\r\n    },\r\n    /**\r\n     * 表单操作\r\n     */\r\n    //增加车辆\r\n    handleAdd() {\r\n      addCar(this.form).then(handleRes(this, \"增加车辆成功\",this.selectCars));\r\n    },\r\n    handleUpdate() {\r\n      updateCar(this.form).then(handleRes(this,\"修改成功\", this.selectCars));\r\n    },\r\n    //添加按钮\r\n    add(){\r\n        handleAddButton(this);\r\n    },\r\n    //修改按钮\r\n    update(row){\r\n      handleUpdateButton(this, row);\r\n    }\r\n  },\r\n  mounted: {\r\n  },\r\n}\r\n</script>\r\n\r\n<style>\r\n.mgb20 {\r\n    margin-bottom: 20px;\r\n}\r\n</style>"],"mappings":"AAmGA,SAAAA,OAAA,EAAAC,QAAA,EAAAC,OAAA,EAAAC,UAAA,EAAAC,SAAA,EAAAC,MAAA,EAAAC,SAAA,EAAAC,SAAA,EAAAC,SAAA;AACA,SAAAC,SAAA,EAAAC,eAAA,EAAAC,kBAAA;AACA;EACAC,KAAA;IACA;MACAC,IAAA;MACAC,MAAA;MACAC,SAAA;MACAD,MAAA,EAAAX,UAAA;MACAa,gBAAA;MACAC,mBAAA;MACAC,IAAA,EAAAhB,OAAA;MACAiB,KAAA;MACAC,OAAA;QACAC,UAAA;QACAC,QAAA;QACAC,SAAA;MACA;IACA;EACA;EACAC,QAAA;IACA,KAAAL,KAAA,GAAAlB,QAAA,MAAAiB,IAAA;IACA,KAAAO,UAAA;EACA;EACAC,OAAA;IACAD,WAAA;MACArB,SAAA,MAAAgB,OAAA,EAAAO,IAAA,CAAAC,GAAA;QACA,KAAAb,SAAA,GAAAa,GAAA,CAAAhB,IAAA,CAAAiB,IAAA;MACA;IACA;IACA;IACAC,WAAA;MACA,KAAAjB,IAAA;MACA,KAAAC,MAAA;IACA;IACA;IACAiB,IAAAC,GAAA;MACA,KAAAC,QAAA;QAAAC,IAAA;MAAA,GAAAP,IAAA;QACApB,SAAA,CAAAyB,GAAA,EAAAL,IAAA,CAAAlB,SAAA,oBAAAgB,UAAA;MACA,GAAAU,KAAA;IACA;IACA;AACA;AACA;IACA;IACAC,UAAA;MACA/B,MAAA,MAAAa,IAAA,EAAAS,IAAA,CAAAlB,SAAA,sBAAAgB,UAAA;IACA;IACAY,aAAA;MACA7B,SAAA,MAAAU,IAAA,EAAAS,IAAA,CAAAlB,SAAA,oBAAAgB,UAAA;IACA;IACA;IACAa,IAAA;MACA5B,eAAA;IACA;IACA;IACA6B,OAAAP,GAAA;MACArB,kBAAA,OAAAqB,GAAA;IACA;EACA;EACAQ,OAAA,GACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}