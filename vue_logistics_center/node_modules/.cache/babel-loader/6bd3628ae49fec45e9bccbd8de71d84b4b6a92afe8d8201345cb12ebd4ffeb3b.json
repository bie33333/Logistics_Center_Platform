{"ast":null,"code":"import { dataRule, dataForm, selectAllUser, addUser, updateUser } from \"@/js/user.js\";\nexport default {\n  data() {\n    return {\n      username: '',\n      tableData: [],\n      dialogVisible: false,\n      updateDialogVisible: false,\n      addDialogVisible: false,\n      form: dataForm(),\n      rules: '',\n      pageSet: {\n        pageNumber: 0,\n        pageSize: 30,\n        pageTotal: 0\n      }\n    };\n  },\n  created() {\n    this.selectUser();\n    this.rules = dataRule(this.form);\n  },\n  methods: {\n    //select用户\n    selectUser() {\n      selectAllUser(this.pageSet).then(res => {\n        this.tableData = res.data.list;\n      });\n    },\n    //重置搜索栏\n    resetParam() {\n      this.username = '';\n    }\n  },\n  mounted: {}\n};","map":{"version":3,"names":["dataRule","dataForm","selectAllUser","addUser","updateUser","data","username","tableData","dialogVisible","updateDialogVisible","addDialogVisible","form","rules","pageSet","pageNumber","pageSize","pageTotal","created","selectUser","methods","then","res","list","resetParam","mounted"],"sources":["src/components/page/Member.vue"],"sourcesContent":["<template>\r\n    <div style=\"margin-bottom: 5px;margin-top: 5px;border-radius: 30%;\">\r\n        <el-input v-model=\"username\" placeholder=\"请输入用户名关键字\" suffix-icon=\"el-icon-search\" style=\"width: 300px;\"\r\n                ></el-input>\r\n        <el-button type=\"success\" style=\"margin-left: 10px;\">搜索</el-button>\r\n        <el-button type=\"info\" @click=\"resetParam\">重置</el-button>\r\n        <el-button size=\"medium\" type=\"primary\" style=\"margin-left: 10px;\" @click=\"addAccount\">增添新账户</el-button>\r\n      \r\n        <div>\r\n          <el-table :data=\"tableData\" :header-cell-style=\"{ background:'orange',color:'black'}\" border>\r\n            <el-table-column prop=\"userName\" label=\"姓名\" width=\"120\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"userAccount\" label=\"账户\" width=\"180\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"userAge\" label=\"年龄\" width=\"100\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"userSex\" label=\"性别\" width=\"100\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"userPhone\" label=\"联系电话\" width=\"180\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"userAddress\" label=\"地址\" width=\"300\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"operate\" label=\"操作\">\r\n              <template slot-scope=\"scope\">\r\n                <el-button type=\"success\" @click=\"getMethod('updateButton',scope.row)\">修改</el-button>\r\n                <el-popconfirm \r\n                    title=\"确认要删除吗?\"\r\n                    @confirm=\"getMethod('delete')\"\r\n                    style=\"margin-left: 10px;\">\r\n                    <el-button slot=\"reference\" type=\"danger\">删除</el-button>\r\n                </el-popconfirm>\r\n              </template>\r\n            </el-table-column>\r\n          </el-table>\r\n        </div>\r\n      \r\n        \r\n        <!-- 增添用户表单 -->\r\n        <el-dialog ref=\"add\"\r\n          title=\"\"\r\n          :visible.sync=\"dialogVisible\"\r\n          width=\"50%\"\r\n          center>\r\n          <el-form ref=\"form\" :rules=\"rules\" :model=\"form\" label-width=\"100px\">\r\n            <el-form-item label=\"用户姓名\" prop=\"userName\">\r\n              <el-input v-model=\"form.userName\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"用户账户\" prop=\"userAccount\">\r\n                <el-input v-model=\"form.userAccount\"></el-input>\r\n              </el-form-item>\r\n            <el-form-item label=\"用户密码\" prop=\"userPassword\" >\r\n              <el-input v-model=\"form.userPassword\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"确认密码\" prop=\"userConfirmedPassword\">\r\n  